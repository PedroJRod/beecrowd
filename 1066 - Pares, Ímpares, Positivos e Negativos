l = []

for i in range(5):
  l.append(int(input()))

pares = 0
impares = 0
positivos = 0
negativos = 0

for i in l:
  if i%2 == 0:
    pares += 1
  elif i%2 != 0:
    impares +=1
  
for i in l:  
  if i>0:
    positivos += 1
  elif i< 0:
    negativos += 1

print(pares, "valor(es) par(es)")
print(impares, "valor(es) impar(es)")
print(positivos, "valor(es) positivo(s)")
print(negativos, "valor(es) negativo(s)")
